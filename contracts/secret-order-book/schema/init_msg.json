{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InitMsg",
  "type": "object",
  "required": [
    "amm_pair_contract_address",
    "amm_pair_contract_hash",
    "factory_address",
    "factory_hash",
    "factory_key",
    "token1_info",
    "token2_info"
  ],
  "properties": {
    "amm_pair_contract_address": {
      "$ref": "#/definitions/HumanAddr"
    },
    "amm_pair_contract_hash": {
      "type": "string"
    },
    "factory_address": {
      "$ref": "#/definitions/HumanAddr"
    },
    "factory_hash": {
      "type": "string"
    },
    "factory_key": {
      "type": "string"
    },
    "token1_info": {
      "$ref": "#/definitions/AssetInfo"
    },
    "token2_info": {
      "$ref": "#/definitions/AssetInfo"
    }
  },
  "definitions": {
    "AssetInfo": {
      "type": "object",
      "required": [
        "is_native_token"
      ],
      "properties": {
        "is_native_token": {
          "type": "boolean"
        },
        "native_token": {
          "anyOf": [
            {
              "$ref": "#/definitions/NativeToken"
            },
            {
              "type": "null"
            }
          ]
        },
        "token": {
          "anyOf": [
            {
              "$ref": "#/definitions/Token"
            },
            {
              "type": "null"
            }
          ]
        }
      }
    },
    "HumanAddr": {
      "type": "string"
    },
    "NativeToken": {
      "type": "object",
      "required": [
        "denom"
      ],
      "properties": {
        "denom": {
          "type": "string"
        }
      }
    },
    "Token": {
      "type": "object",
      "required": [
        "contract_addr",
        "token_code_hash"
      ],
      "properties": {
        "contract_addr": {
          "$ref": "#/definitions/HumanAddr"
        },
        "token_code_hash": {
          "type": "string"
        }
      }
    }
  }
}
